{
  "name": "parcel_boilerplate",
  "version": "1.0.0",
  "description": "A parcel boilerplate optimized for production build.",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/index.html",
    "./app.mjs"
  ],
  "scripts": {
    "start": "conc --kill-others \"npm run express-development\" \"npm run development\"",
    "development": "parcel",
    "build": "parcel build",
    "express-build": "node app.mjs",
    "express-development": "nodemon app.mjs"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.7.1",
    "concurrently": "^7.6.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.33.0",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "nodemon": "^2.0.20",
    "os-browserify": "^0.3.0",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@prismicio/client": "^6.7.3",
    "@prismicio/helpers": "^2.3.9",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gsap": "^3.11.4",
    "node-fetch": "^3.3.0"
  }
}
